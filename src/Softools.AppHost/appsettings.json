{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Parameters": {
    "RabbitMqUsername": "admin",
    "RabbitMqPassword": "admin"
  },
  "Jwt": {
    "Key": "ESSA É APENAS UMA STRING PARA DESENVOLVIMENTO E SERA ALTERADA PARA UMA STRING MAIS SEGURA. NÃO USE ISSO EM PRODUÇÃO",
    "Issuer": "softools",
    "Audience": "softools"
  },

  "ReverseProxy": {
    "Routes": {
      "projetos": {
        "ClusterId": "projetos",
        "Match": {
          "Path": "/api/projetos/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api"
          }
        ]
      },
      "usuarios": {
        "ClusterId": "usuarios",
        "Match": {
          "Path": "/api/usuarios/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api"
          }
        ]
      },
      "documentos": {
        "ClusterId": "documentos",
        "Match": {
          "Path": "/api/documentos/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/api"
          }
        ]
      }
    },
    "Clusters": {
      "projetos": {
        "Destinations": {
          "projetos": {
            "Address": "http://projetos"
          }
        }
      },
      "usuarios": {
        "Destinations": {
          "usuarios": {
            "Address": "http://usuarios"
          }
        }
      },
      "documentos": {
        "Destinations": {
          "documentos": {
            "Address": "http://documentos"
          }
        }
      }
    }
  }
}
